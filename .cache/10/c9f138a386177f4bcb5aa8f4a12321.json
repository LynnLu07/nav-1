r\n        max-width: 400px;\r\n        margin-left: auto;\r\n        margin-right: auto;        \r\n    }\r\n}\r\n\r\n.searchForm > input{\r\nwidth: 100%;\r\nmargin-right: 10px;\r\nheight: 40px;\r\npadding: 0 10px;\r\nborder: 1px solid #ddd;\r\nborder-radius: 4px;\r\n}\r\n.searchForm > button{\r\nwhite-space: nowrap;\r\npadding: 0 25px;\r\nborder: none;\r\nborder-radius: 4px;\r\nbackground: #3EB4E7;\r\ncolor:white;\r\nfont-size: 16px;\r\n}\r\n\r\n.globalMain{\r\n    max-width: 900px;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n}\r\n\r\n.siteList{\r\n    margin: 16px;\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-between;\r\n}\r\n\r\n@media(min-width:500px){\r\n  .siteList{\r\n      margin-left: 0;\r\n      margin-right: -25px;\r\n      justify-content: flex-start;\r\n  }\r\n}\r\n  .siteList > li{\r\n  margin-bottom: 20px;\r\n}\r\n@media(min-width:500px){\r\n    .siteList > li {\r\n    margin-right: 25px;\r\n    }\r\n}\r\n.siteList .site{\r\nwidth: 160px;\r\ndisplay: flex;\r\njustify-content: center;\r\nflex-direction: column;\r\nalign-items: center;\r\nbackground:rgba(255,255,255,0.5);\r\n/* border: 1px solid #ddd; */\r\nborder-radius: 6px;\r\npadding: 20px 0;\r\n/* margin-bottom: 10px; */\r\nposition: relative;\r\ncursor: pointer;\r\n}\r\n\r\n.siteList .site > .logo{\r\nwidth: 64px;\r\nheight: 64px;\r\ndisplay: flex;\r\njustify-content: center;\r\nalign-items: center;\r\nfont-size: 64px;\r\ntext-transform: uppercase;\r\n}\r\n.siteList .site > .link{\r\nfont-size: 14px;\r\nmargin-top: 4px;\r\n}\r\n.siteList .site > .close{\r\n    position: absolute;\r\n    left: 140px;\r\n    bottom: 40px;\r\n    display: none;\r\n    cursor: default;\r\n}\r\n.siteList .site:hover > .close{\r\n    display: block;\r\n\r\n}\r\n.siteList .addButton{\r\nwidth: 160px;\r\nbackground:rgba(255,255,255,0.5);\r\n/* border: 1px solid #dddddd; */\r\ndisplay: flex;\r\njustify-content: center;\r\nalign-items: center;\r\nflex-direction: column;\r\npadding: 20px 0;\r\n}\r\n.siteList .addButton .icon{\r\nwidth: 50px;\r\nheight: 50px;\r\ndisplay: flex;\r\njustify-content: center;\r\nalign-items: center;\r\nflex-direction: column;\r\n}\r\n.siteList .addButton .text{\r\nfont-size: 14px;\r\nmargin-top: 4px;\r\n}\r\n\r\n.siteList .addButton .icon-wrapper{\r\nwidth: 64px;\r\nheight: 64px;\r\ndisplay: flex;\r\njustify-content: center;\r\nalign-items: center;\r\nflex-direction: column;\r\n}"},"lineCount":null}},"error":null,"hash":"2355767c759eeea1103271739bdf0961","cacheData":{"env":{}}}